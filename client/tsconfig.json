{
	"compilerOptions": {
		"target": "es2017",
		"lib": [
			"dom",
			"dom.iterable",
			"esnext",
			"webworker"
		],
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": "src",
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "react",
		"importHelpers": true
	},
	"include": [
		"src"
	]
}
